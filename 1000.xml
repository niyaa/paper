<NEKTAR>
    <EXPANSIONS>
        <E COMPOSITE="C[10]" NUMMODES="6" TYPE="GLL_LAGRANGE" FIELDS="u,v,w,p" />
    </EXPANSIONS>

    <CONDITIONS>
        <SOLVERINFO>
          <I PROPERTY="SolverType" VALUE="VelocityCorrectionScheme" />
          <I PROPERTY="EQTYPE" VALUE="UnsteadyNavierStokes" />
          
          <I PROPERTY="Projection" VALUE="Galerkin" />
          <I PROPERTY="TimeIntegrationMethod" VALUE="IMEXOrder2" />
          <I PROPERTY="HOMOGENEOUS" VALUE="1D" />
          <I PROPERTY="USEFFT" VALUE="FFTW" />
        </SOLVERINFO>

        <PARAMETERS>
          <P>TimeStep =0.005</P>
          <P>FinalTime =150</P>
          <P> NumSteps      = FinalTime/TimeStep </P>
          <P>IO_CheckSteps =FinalTime/TimeStep</P>
          <P> IO_InfoSteps   = 500       </P>
          <P>Re=2000</P>
          <P> Kinvis        = 1/Re   </P>
          <P> HomModesZ      = 4        </P>
          <P>LZ=14.9599650171</P>
        </PARAMETERS>
    
        <VARIABLES>
          <V ID="0">u</V>
          <V ID="1">v</V>
          <V ID="2">w</V>
          <V ID="3">p</V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[7] </B>  
            <B ID="1"> C[8] </B>  
            <B ID="2"> C[9] </B>  
        </BOUNDARYREGIONS>

          <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <D VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
                <D VAR="w" VALUE="0" />
                <N VAR="p" USERDEFINEDTYPE="H" VALUE="0" />
            </REGION>
            <REGION REF="1">
                <P VAR="u" VALUE="[2]" />
                <P VAR="v" VALUE="[2]" />
                <P VAR="w" VALUE="[2]" />
                <P VAR="p" VALUE="[2]" />
            </REGION>
            <REGION REF="2">
                <P VAR="u" VALUE="[1]" />
                <P VAR="v" VALUE="[1]" />
                <P VAR="w" VALUE="[1]" />
                <P VAR="p" VALUE="[1]" />
            </REGION>
        </BOUNDARYCONDITIONS>
        
        <FUNCTION NAME="BodyForce">
          <E VAR="u" VALUE="0" />
          <E VAR="v" VALUE="0" />
          <E VAR="w" VALUE="2*Kinvis" />
        </FUNCTION>
        
        <FUNCTION NAME="InitialConditions">
          <F FILE="geomHplusD.fld" />
        </FUNCTION>
    </CONDITIONS>
    
    <FORCING>
        <FORCE TYPE="Body">
            <BODYFORCE> BodyForce </BODYFORCE>
        </FORCE>
    </FORCING>

    <FILTERS>
        <FILTER TYPE="ModalEnergy">
	        <PARAM NAME="OutputFile">1.0-0.6-0.44-108-.mdl</PARAM>
            <PARAM NAME="OutputFrequency">0.5/TimeStep</PARAM>
        </FILTER>
       <FILTER TYPE="HistoryPoints">
          <PARAM NAME="OutputFile">TimeValues</PARAM>
          <PARAM NAME="OutputFrequency">0.1/TimeStep</PARAM>
          <PARAM NAME="OutputPlane">2</PARAM>
          <PARAM NAME="Points">
3.14159265359 0 0 
</PARAM>
                </FILTER>

                    </FILTERS>

</NEKTAR>